//hypens
import java.util.*;
class main
{
public static void main (String[] args)
{
Scanner sc = new Scanner (System.in);
{
        String s=sc.nextLine();
        String res=""; 
        for(int i=0;i<s.length();i++)
        {
            if(s.charAt(i)=='-')
            {
                res=s.charAt(i)+res;
            }
            else{
                res=res+s.charAt(i);
            }
        }
        System.out.print(res);
    }
}
}
